{
    "name": "high-five/component-library",
    "description": "Component library module for Craft CMS websites",
    "type": "craft-module",
    "version": "0.0.0",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "High Five",
            "email": "hello@high-five.dev",
            "homepage": "https://www.high-five.dev"
        },
        {
            "name": "Harm Putman",
            "email": "harm@high-five.dev",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@high-five.dev"
    },
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "require-dev": {
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "HighFive\\ComponentLibrary\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        }
    },
    "scripts": {
        "phpstan": "phpstan --memory-limit=1G"
    }
}
